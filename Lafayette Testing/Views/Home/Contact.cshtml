    ViewBag.Title = "Contact Us";
	ViewBag.Description = "";
	ViewBag.BodyClass = "contact";
}

<section class="section row">
	<div id="primary">
		<nav class="breadcrumb col sm-12" itemtype="http://data-vocabulary.org/Breadcrumb" itemscope="">
			<h6 class="assistive-text">Breadcrumb navigation</h6>
			<ol>
				<li><a itemprop="url" href="/"><span itemprop="title">Home</span></a></li>
				<li><span itemprop="title">@ViewBag.Title</span></li>
			</ol>
		</nav>
